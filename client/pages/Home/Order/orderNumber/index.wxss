page{  
  display: flex;  
  flex-direction: column;  
  height: 100%; 
  background-color: #f9f9f9; 
}  
.navbar{  
  flex: none;  
  display: flex;  
  background: #fff;  
}  
.navbar .item{  
  position: relative;  
  flex: auto;  
  text-align: center;  
  line-height: 80rpx;
  font-size:32rpx;   
}  
.navbar .item.active{  
  color: #f58623;
  font-size:32rpx;
}  
.navbar .item.active:after{  
  content: "";  
  display: block;  
  position: absolute;  
  bottom: 0;  
  left: 0;  
  right: 0;  
  height: 4rpx;  
  background:OrangeRed;  
}

.page{
  width: 100%;
  height: 1200rpx;
  background: #fff;
}
.carte{
  width: 100%;
  height: 400rpx;
}
.carte-info{
  width: 100%;
  height: 150rpx;
  border-bottom: 1rpx solid #f4f4f4;
}
.carte-name{
  padding-top: 30rpx;
  padding-left: 40rpx;
  color: #666;
  font-size:30rpx;
  font-weight: bold;
}
.carte-price{
  margin-top: 13rpx;
  padding-left: 34rpx;
  color: #f47d12;
  font-size:30rpx;
  font-weight: bold;
  margin-bottom:0;
}
.shop-info{
  color: #666;
  font-size:30rpx;
  font-weight: bold;
  padding-left: 40rpx;
}
.shop-view{
  width: 88%;
  color: #666;
  font-size:30rpx;
  padding-left: 40rpx;
  text-indent:15px;
  word-wrap:break-word;
  padding-top: 10rpx;
  line-height: 50rpx;
}
.intodiancan{
  position: fixed;
  bottom: 0;
  width: 100%;
  border: none;
  background: linear-gradient(to right, #FAD35C , #F76b1C);
  color: white;
}